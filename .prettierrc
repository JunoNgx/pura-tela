{
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "printWidth": 100,
    "bracketSpacing": true,
    "experimentalOperatorPosition": "start",
    "plugins": ["prettier-plugin-svelte"],
    "bracketSameLine": false,
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
